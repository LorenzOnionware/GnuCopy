<Application
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Project1"
    xmlns:style="using:FluentAvalonia.Styling"
    xmlns:converters="clr-namespace:Project1.converters"
    x:Class="Project1.App">
    <Application.Styles>
        <style:FluentAvaloniaTheme PreferUserAccentColor="True"/>
    </Application.Styles>
    <Application.Resources>
        <converters:ListingConverter x:Key="ListingConverter"/>

    </Application.Resources>
</Application>
